{
  "name": "service-exchange-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "web-bundle": "webpack-cli --config webpack.prod.config.js --progress --colors",
    "web-dev": "webpack-dev-server --display-error-details --content-base public/ --config webpack.dev.config.js --port 3002 --inline --hot --colors --mode development --history-api-fallback",
    "start": "npm run server",
    "server": "node --harmony server.js",
    "dev": "webpack-dev-server --display-error-details --config ./webpack.dev.config.js --mode development --hot --history-api-fallback --open --host 0.0.0.0",
    "prebuild": "rimraf public",
    "build:prod": "npm run prebuild && webpack --config ./webpack.prod.config.js --mode production --progress --profile --colors",
    "build:dev": "npm run prebuild && webpack --config ./webpack.dev.config.js --mode development --progress --profile --colors",
    "format": "prettier --print-width 100 --single-quote --write 'src/**/*.js'",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc.json src",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.22",
    "apollo-link-rest": "^0.7.0",
    "axios": "^0.18.0",
    "evergreen-ui": "^4.8.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "jss-vendor-prefixer": "^8.0.1",
    "material-ui-chip-input": "^1.0.0-beta",
    "material-ui-superselectfield": "^1.9.8",
    "material-ui-time-picker": "^1.1.0",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-state-router": "^4.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0-alpha.2",
    "react-hot-loader": "4.6.0",
    "react-jss": "^8.6.1",
    "react-select": "^2.1.2",
    "react-styleguidist": "^8.0.6",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-decorators": "7.2.2",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cypress": "^3.1.3",
    "eslint": "^5.10.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.6.0",
    "json-loader": "^0.5.7",
    "mobx-logger": "^0.7.1",
    "mobx-react-devtools": "^6.0.3",
    "react-test-renderer": "16.6.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
